{
  "version": "1.0",
  "description": "Poky reference distribution for scarthgap-wip",
  "sources": {
    "oe-layersetup": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.ti.com/git/arago-project/oe-layersetup.git"
          }
        },
        "rev": "master"
      },
      "path": "oe-layersetup"
    },
    "bitbake": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.openembedded.org/bitbake"
          }
        },
        "rev": "2.8"
      },
      "path": "bitbake"
    },
    "meta-yocto": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.yoctoproject.org/meta-yocto"
          }
        },
        "rev": "scarthgap"
      },
      "path": "meta-yocto"
    },
    "meta-ti": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.yoctoproject.org/meta-ti"
          }
        },
        "rev": "scarthgap-wip"
      },
      "path": "meta-ti"
    },
    "meta-arm": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.yoctoproject.org/meta-arm"
          }
        },
        "rev": "scarthgap"
      },
      "path": "meta-arm"
    },
    "oe-core": {
      "git-remote": {
        "remotes": {
          "origin": {
            "uri": "https://git.openembedded.org/openembedded-core"
          }
        },
        "rev": "scarthgap"
      },
      "path": "oe-core"
    }
  },
  "configuration": {
    "bitbake-setup": {
      "default": {
        "description": "Default build configuration",
         "template": "oe-layersetup-poky-scarthgap-wip-config",
         "targets": [ "bitbake -k core_image_minimal" ]
      }
    }
  }
}
